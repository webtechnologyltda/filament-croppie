{
    "name": "webtechnologyltda/filament-croppie",
    "description": "Croppie Js Implementation Field for Filament forms",
    "keywords": [
        "filament",
        "webtechnologyltda",
        "field",
        "image",
        "croppie",
        "cropperjs",
        "croppie-js",
        "croppie-image",
        "image-resize",
        "croppie-image-resize"
    ],
    "homepage": "https://github.com/webtechnologyltda/filament-croppie",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Douglas",
            "email": "usermich999@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "illuminate/contracts": "^10.0|^11.0",
        "league/commonmark": "^2.4",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Michaeld555\\FilamentCroppie\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michaeld555\\FilamentCroppie\\FilamentCroppieServiceProvider"
            ],
            "aliases": {
                "Croppie": "Michaeld555\\FilamentCroppie\\Facades\\FilamentCroppie"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
